{
  "mcpServers": {
    "chrome-devtools": {
      "type": "local",
      "command": "npx",
      "tools": [
        "*"
      ],
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ]
    },
    "x-ipe-app-and-agent-interaction": {
      "type": "local",
      "command": "uv",
      "tools": [
        "*"
      ],
      "args": [
        "run",
        "--directory",
        "${workspaceFolder}",
        "python",
        "-m",
        "x_ipe.mcp.app_agent_interaction"
      ]
    }
  }
}